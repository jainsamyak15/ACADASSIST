{
    "version": 2,
    "builds": [
      {
        "src": "**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)\\.html",
        "dest": "/$1.html"
      },
      {
        "src": "/(.*)\\.css",
        "dest": "/$1.css"
      },
      {
        "src": "/(.*)\\.js",
        "dest": "/$1.js"
      },
      {
        "src": "/images/(.*)",
        "dest": "/images/$1"
      },
      {
        "src": "/components/(.*)",
        "dest": "/components/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/js/$1"
      },
      {
        "src": "/admin/(.*)",
        "dest": "/admin/$1"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/about",
        "dest": "/about.html"
      },
      {
        "src": "/applications",
        "dest": "/applications.html"
      },
      {
        "src": "/resources",
        "dest": "/resources.html"
      },
      {
        "src": "/hackathons",
        "dest": "/hackathons.html"
      },
      {
        "src": "/internship",
        "dest": "/internship.html"
      },
      {
        "src": "/scholarships",
        "dest": "/scholarships.html"
      },
      {
        "src": "/mentorship",
        "dest": "/mentorship.html"
      },
      {
        "src": "/contact",
        "dest": "/contact.html"
      }
    ]
  }