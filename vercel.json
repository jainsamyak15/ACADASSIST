{
    "version": 2,
    "builds": [
      {
        "src": "**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/admin",
        "dest": "/admin/index.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/images/(.*)",
        "dest": "/images/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/js/$1"
      },
      {
        "src": "/components/(.*)",
        "dest": "/components/$1"
      },
      {
        "src": "/admin/(.*)",
        "dest": "/admin/$1"
      },
      {
        "src": "/(.*)\\.js",
        "headers": {
          "content-type": "application/javascript"
        },
        "dest": "/$1.js"
      },
      {
        "src": "/(.*)\\.css",
        "headers": {
          "content-type": "text/css"
        },
        "dest": "/$1.css"
      },
      {
        "src": "/(.*)\\.html",
        "dest": "/$1.html"
      },
      {
        "src": "/about",
        "dest": "/about.html"
      },
      {
        "src": "/applications",
        "dest": "/applications.html"
      },
      {
        "src": "/resources",
        "dest": "/resources.html"
      },
      {
        "src": "/hackathons",
        "dest": "/hackathons.html"
      },
      {
        "src": "/internship",
        "dest": "/internship.html"
      },
      {
        "src": "/scholarships",
        "dest": "/scholarships.html"
      },
      {
        "src": "/mentorship",
        "dest": "/mentorship.html"
      },
      {
        "src": "/contact",
        "dest": "/contact.html"
      },
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/404.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-XSS-Protection",
            "value": "1; mode=block"
          }
        ]
      }
    ]
  }